{
  "name": "flox-react",
  "version": "1.0.0",
  "type": "module",
  "description": "React State Management inspired by GetX Flutter - Flox",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.esm.js",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:lib": "rollup -c",
    "preview": "vite preview",
    "prepublishOnly": "npm run build:lib"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.8",
    "@rollup/plugin-node-resolve": "^15.3.1",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "rollup": "^4.46.2",
    "rollup-plugin-dts": "^6.2.1",
    "tslib": "^2.8.1",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  },
  "files": [
    "dist",
    "README.md",
    "GETTING_STARTED.md",
    "CHEAT_SHEET.md",
    "INSTALLATION.md"
  ],
  "keywords": [
    "react",
    "state-management",
    "typescript",
    "getx",
    "flutter",
    "reactive",
    "controller",
    "binding"
  ],
  "author": "Flox Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/flox.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/flox/issues"
  },
  "homepage": "https://github.com/your-username/flox#readme"
}
